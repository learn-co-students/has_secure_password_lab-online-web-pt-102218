<%= form_for :user, url: '/users' do |f| %>
  <%= f.label "Username:" %>
  <%= f.text_field :username %> <br>

  <%= f.label "Password" %>
  <%= f.password_field :password %>
  <%= f.label "Password Confirmation" %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Submit" %>

<% end %> 
